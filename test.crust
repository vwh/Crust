set person = {
  name: "Yazan",
  someThing: null,
  isGay: true,
  nested: {
    age: 69,
  },
};

fn makeAdder(offset) {
  fn add(x, y) {
    set z = x + y + offset;
    z
  }

  add
}

fn getName(person) {
  person.name
}

set add = makeAdder(2);
keep y = add(5, 5) + person.nested.age;

output(y)
output(getName(person))
